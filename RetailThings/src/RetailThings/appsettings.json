{
  "Application": {},
  "Infrastructure": {
    "Persistence": {
      "Postgres": {
        "Host": "localhost",
        "Port": "5432",
        "Database" : "RetailThings",
        "Username": "postgres",
        "Password": "postgres"
      }
    }
  },
  "Presentation": {
  },
  "Platform": {
    "Environment": ""
  },
  "Sentry": {
    "Enabled": true,
    "Environment": "",
    "Dsn": "",
    "Debug": false,
    "TracesSampleRate": 1.0,
    "MinimumEventLevel": "Warning"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "HTTP": {
        "Url": "https://localhost:5017"
      }
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Extensions.Http": "Information",
        "Grpc.AspNetCore.Server": "Information",
        "Grpc.Net.Client": "Information",
        "System.Net.Http.HttpClient": "Information",
        "Npgsql.Command": "Warning",
        "Npgsql.Connection": "Warning",
        "Npgsql.Transaction": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:T} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs/RetailThings/AppLogs_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] {SourceContext} {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 30
        }
      }
    ]
  }

}
